{
  "manifest_version": 3,
  "name": "walk-updater",
  "version": "1.0",
  "description": "",
  "icons": {
    "48": "icons/icon.png"
  },
  "permissions": [
    "activeTab",
    "menus",
    "scripting",
    "storage"
  ],
  "options_ui": {
    "page": "options/options.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "action": {
    "default_icon": "icons/icon.png",
    "default_title": "Walk Updater",
    "default_popup": "browserAction/popup.html"
  },
  "commands": {
    "run-action": {
      "suggested_key": {
        "default": "Ctrl+Alt+1"
      },
      "description": "Run action"
    }
  }
}
